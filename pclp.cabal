name: pclp
version: 0.0.1
cabal-version: >=1.4
build-type: Simple
license: AllRightsReserved

executable pclp
    main-is: Main.hs
    cpp-options: -DMAIN_FUNCTION=MainPCLP.main
    build-depends:
        --QuickCheck -any,
        base -any,
        containers -any,
        explicit-exception -any,
        hashable -any,
        parsec -any,
        statistics -any,
        time -any,
        unordered-containers -any,
        mtl -any
    extensions: StandaloneDeriving GADTs DeriveGeneric TupleSections EmptyDataDecls
    hs-source-dirs: src
    other-modules:
        MainPCLP
        GWMCExact
        Parser
        PST
        Formula
        GWMC
        AST
        BasicTypes
        Benchmarks
        Grounder
        Interval
        Exception
    ghc-options: -rtsopts

